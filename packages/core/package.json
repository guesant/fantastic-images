{
  "version": "0.0.1",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "start": "tsc -p tsconfig.json --declaration --watch",
    "build": "tsc -p tsconfig.json --declaration",
    "test": "jest src",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run format"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@fantastic-images/core",
  "author": "Gabriel Rodrigues",
  "module": "dist/index.js",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@fantastic-images/types": "file:../types",
    "@types/fabric": "^3.6.8",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "babel-jest": "^26.3.0",
    "eslint": "^7.9.0",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "prettier": "^2.1.2",
    "tslib": "^2.0.1",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@fantastic-images/lib": "file:../lib"
  }
}
